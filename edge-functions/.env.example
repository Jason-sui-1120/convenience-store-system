# EdgeOne边缘函数环境变量配置

# MongoDB Atlas连接字符串
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/inventory?retryWrites=true&w=majority

# 运行环境
NODE_ENV=production

# 应用配置
APP_NAME=inventory-management
APP_VERSION=1.0.0

# 安全配置
JWT_SECRET=your-jwt-secret-key
BCRYPT_ROUNDS=12

# 限流配置
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# 日志配置
LOG_LEVEL=info